{
    "settings": {
        "index": {
            "number_of_shards": 1,
            "number_of_replicas": 0
        }
    },
    "mappings": {
        "_default_": {
            "_all": {"enabled" : false},
            "dynamic": "strict",
            "properties": {
                "created_at": {
                    "type": "date",
                    "format": "date_optional_time"
                },
                "processed_at": {
                    "type": "date",
                    "format": "date_optional_time"
                },
                "user_properties": {
                    "type": "object",
                    "properties": {
                        "user_agent": {
                            "type": "string",
                            "index": "not_analyzed"
                        },
                        "referer": {
                            "type": "string",
                            "index": "not_analyzed"
                        },
                        "ip": {
                            "type": "string",
                            "index": "not_analyzed"
                        }
                    }
                }
            }
        },
        "search": {
            "dynamic_templates": [
                {
                    "query_properties": {
                        "path_match": "event_properties.query.*",
                        "match_mapping_type": "string",
                        "mapping": {
                            "type": "string",
                            "index": "not_analyzed"
                        }
                    }
                }
            ],
            "properties": {
                "event_properties": {
                    "type": "object",
                    "properties": {
                        "source_id": {
                            "type": "string",
                            "index": "not_analyzed"
                        },
                        "query": {
                            "dynamic": true,
                            "type": "object"
                        },
                        "hits": {
                            "properties": {
                                "source_id": {
                                    "type": "string",
                                    "index": "not_analyzed"
                                },
                                "object_id": {
                                    "type": "string",
                                    "index": "not_analyzed"
                                },
                                "score": {
                                    "type": "float"
                                }
                            }
                        },
                        "n_total_hits": {
                            "type": "integer"
                        },
                        "query_time_ms": {
                            "type": "integer"
                        }
                    }
                }
            }
        },
        "search_similar": {
            "properties": {
                "event_properties": {
                    "type": "object",
                    "properties": {
                        "similar_to_object": {
                            "type": "object",
                            "properties": {
                                "source_id": {
                                    "type": "string",
                                    "index": "not_analyzed"
                                },
                                "object_id": {
                                    "type": "string",
                                    "index": "not_analyzed"
                                }
                            }
                        },
                        "query": {
                            "dynamic": true,
                            "type": "object"
                        },
                        "hits": {
                            "properties": {
                                "source_id": {
                                    "type": "string",
                                    "index": "not_analyzed"
                                },
                                "object_id": {
                                    "type": "string",
                                    "index": "not_analyzed"
                                },
                                "score": {
                                    "type": "float"
                                }
                            }
                        },
                        "n_total_hits": {
                            "type": "integer"
                        },
                        "query_time_ms": {
                            "type": "integer"
                        }
                    }
                }
            }
        },
        "sources": {
            "properties": {
                "event_properties": {
                    "type": "object",
                    "properties": {
                        "query_time_ms": {
                            "type": "integer"
                        }
                    }
                }
            }
        },
        "get_object": {
            "properties": {
                "event_properties": {
                    "type": "object",
                    "properties": {
                        "source_id": {
                            "type": "string",
                            "index": "not_analyzed"
                        },
                        "object_id": {
                            "type": "string",
                            "index": "not_analyzed"
                        }
                    }
                }
            }
        },
        "get_object_source": {
            "properties": {
                "event_properties": {
                    "type": "object",
                    "properties": {
                        "source_id": {
                            "type": "string",
                            "index": "not_analyzed"
                        },
                        "object_id": {
                            "type": "string",
                            "index": "not_analyzed"
                        }
                    }
                }
            }
        },
        "resolve": {
            "properties": {
                "event_properties": {
                    "type": "object",
                    "properties": {
                        "url_id": {
                            "type": "string",
                            "index": "not_analyzed"
                        }
                    }
                }
            }
        }
    }
}